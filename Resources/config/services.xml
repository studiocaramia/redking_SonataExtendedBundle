<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    
    <parameters>
        <parameter key="redking_sonata_extended.form.info.class">Redking\Bundle\SonataExtendedBundle\Form\Type\InfoType</parameter>
        <parameter key="redking_sonata_extended.form.taginput.class">Redking\Bundle\SonataExtendedBundle\Form\Type\TagInputType</parameter>
    </parameters>

    <services>
        <service id="redking_sonata_extended.form.info" class="%redking_sonata_extended.form.info.class%">
            <tag name="form.type" alias="info"/>
        </service>
        <service id="redking_sonata_extended.form.taginput" class="%redking_sonata_extended.form.taginput.class%">
            <tag name="form.type" alias="taginput"/>
        </service>
    </services>
    
</container>

{
    "name": "redking/sonata-extended-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 2 ",
    "keywords": [ "symfony2", "sonata"],
    "homepage": "https://bitbucket.org/redkingteam/redkingsonataextendedbundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Matabon",
            "homepage": "http://www.makemyweb.fr/",
            "email": "damien.matabon@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "sonata-project/admin-bundle": "2.3.*@dev",
        "sonata-project/intl-bundle": "2.2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": { "Redking\\Bundle\\SonataExtendedBundle": "" }
    },
    "target-dir": "Redking/Bundle/SonataExtendedBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
